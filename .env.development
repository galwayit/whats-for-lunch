# Development environment configuration
APP_ENVIRONMENT=development
ENABLE_AI_FEATURES=true
ENABLE_PLACES_API=true
ENABLE_DEBUG_LOGGING=true
ENABLE_USAGE_MONITORING=true
ENABLE_COST_ALERTS=true
ENABLE_FALLBACK_MODE=true

# API Configuration (using demo keys for development)
GEMINI_API_KEY=demo_key_replace_in_production
GOOGLE_PLACES_API_KEY=demo_key_replace_in_production
GEMINI_MODEL=gemini-1.5-flash
GEMINI_MAX_TOKENS=1000
GEMINI_MAX_REQUESTS_PER_MINUTE=10
GEMINI_ENABLE_CACHING=true
GEMINI_CACHE_TTL_HOURS=24

# Rate Limiting
GOOGLE_PLACES_MAX_REQUESTS_PER_MINUTE=60
GOOGLE_PLACES_DAILY_QUOTA_LIMIT=10000
API_TIMEOUT_SECONDS=30

# Security (development keys only)
ALLOWED_BUNDLE_IDS=com.example.whatwehaveforlunch
ALLOWED_PACKAGE_NAMES=com.example.whatwehaveforlunch
API_ENCRYPTION_KEY=dev_key_only_not_for_production_use_123

# Monitoring and Alerts
ALERT_WEBHOOK_URL=